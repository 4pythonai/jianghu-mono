.page-container {
	min-height: 100vh;
	background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
	display: flex;
	align-items: center;
	justify-content: center;
}

.empty-state {
	text-align: center;
	color: white;
}

.empty-text {
	font-size: 48rpx;
	font-weight: 500;
	margin-bottom: 20rpx;
}

.empty-desc {
	font-size: 28rpx;
	opacity: 0.8;
}
