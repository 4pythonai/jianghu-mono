.container {
	min-height: 100vh;
	background-color: #ffffff;
}

.header {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 88rpx;
	display: flex;
	align-items: center;
	padding: 0 30rpx;
	background-color: #ffffff;
	border-bottom: 1rpx solid #f0f0f0;
	z-index: 100;
}

.back-btn {
	width: 60rpx;
	height: 60rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

.back-icon {
	font-size: 40rpx;
	color: #333;
}

.title {
	margin-left: 20rpx;
	font-size: 32rpx;
	font-weight: 500;
	color: #333;
}

.content {
	margin-top: 88rpx;
	padding: 30rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	height: calc(100vh - 88rpx);
}

.build-text {
	font-size: 36rpx;
	color: #999;
}
