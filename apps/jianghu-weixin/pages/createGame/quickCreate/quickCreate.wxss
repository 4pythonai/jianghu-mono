.container {
	min-height: 100vh;
	background-color: #ffffff;
	padding-top: 200rpx; /* 预留导航栏空间 */
}

.header {
	display: none; /* 使用 CustomNavBar 替代 */
}

.content {
	padding: 30rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	height: calc(100vh - 200rpx);
}

.build-text {
	font-size: 36rpx;
	color: #999;
}
