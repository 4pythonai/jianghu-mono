.container {
    padding: 32rpx;
    background-color: #f5f5f5;
    min-height: 100vh;
}

.debug-info {
    background-color: #fff3cd;
    border: 2rpx solid #ffeaa7;
    border-radius: 8rpx;
    padding: 24rpx;
    margin-bottom: 32rpx;
}

.debug-title {
    font-weight: bold;
    color: #856404;
    display: block;
    margin-bottom: 12rpx;
}

.debug-content {
    color: #856404;
    font-family: monospace;
}

.page-content {
    background-color: white;
    border-radius: 16rpx;
    padding: 32rpx;
}

.page-header {
    text-align: center;
    margin-bottom: 48rpx;
}

.page-title {
    font-size: 36rpx;
    font-weight: bold;
    color: #333;
}

.placeholder {
    text-align: center;
    padding: 80rpx 0;
}

.placeholder-text {
    color: #999;
    font-size: 28rpx;
}