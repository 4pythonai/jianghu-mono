/* ===== 兼容性处理 ===== */

/* 确保在微信小程序中正确显示 */
button.btn {
	line-height: 1;
}

/* 处理微信小程序按钮的默认样式 */
button.btn {
	background-color: transparent;
}

button.btn::after {
	border: none;
}

/* ===== 工具类 ===== */

/* 文本按钮 */
.btn-text {
	background: none;
	color: #007aff;
	padding: 0;
	height: auto;
}

.btn-text:active {
	opacity: 0.7;
}

/* 链接按钮 */
.btn-link {
	background: none;
	color: #007aff;
	text-decoration: underline;
	padding: 0;
	height: auto;
}

.btn-link:active {
	color: #0056b3;
}

